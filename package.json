    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.36.4",
    "drizzle-zod": "^0.8.3",
    "fastify": "^5.1.0",
    "fastify-plugin": "^5.0.1",
    "fastify-zod-openapi": "^5.5.0",
